<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NewCoverView">
        <attr name="new_cover" format="reference" />
        <attr name="new_frame" format="reference" />
        <attr name="new_rad" format="dimension" />
        <attr name="new_circle" format="boolean" />
    </declare-styleable>

    <!--动态加载框-->
    <declare-styleable name="SVCircleProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="maxp" format="integer"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
</resources>